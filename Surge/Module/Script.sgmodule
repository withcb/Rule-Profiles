#!name=Script
#!desc=脚本
#!system=ios

[Script]
# 京东历史价格
jd_price.js = script-path=https://raw.githubusercontent.com/yichahucha/surge/master/jd_price.js,requires-body=1,type=http-response,pattern=^https?://api\.m\.jd\.com/client\.action\?functionId=(wareBusiness|serverConfig)
# Termius
Termius.js = requires-body=1,max-size=0,script-path=https://raw.githubusercontent.com/NobyDa/Script/master/Surge/JS/Termius.js,type=http-response,pattern=https:\/\/api\.termius\.com\/api\/v3\/bulk\/account\/
# 微博去广告
wb_ad.js = script-path=https://raw.githubusercontent.com/yichahucha/surge/master/wb_ad.js,requires-body=true,type=http-response,pattern=^https?://m?api\.weibo\.c(n|om)/2/(statuses/(unread|extend|positives/get|(friends|video)(/|_)timeline)|stories/(video_stream|home_list)|(groups|fangle)/timeline|profile/statuses|comments/build_comments|photo/recommend_list|service/picfeed|searchall|cardlist|page)
# 微博去广告
wb_launch.js = script-path=https://raw.githubusercontent.com/yichahucha/surge/master/wb_launch.js,requires-body=true,type=http-response,pattern=^https?://(sdk|wb)app\.uve\.weibo\.com(/interface/sdk/sdkad.php|/wbapplua/wbpullad.lua)
# 微信读书会员
weixinyuedu.js = script-path=https://raw.githubusercontent.com/imxh/js/master/weixinyuedu.js,requires-body=true,type=http-response,pattern=https?:\/\/i\.weread\.qq\.com\/pay\/memberCardSummary
# 微信公众号文章去广告
Wechat.js = requires-body=1,max-size=0,script-path=https://raw.githubusercontent.com/NobyDa/Script/master/QuantumultX/File/Wechat.js,type=http-response,pattern=^https?:\/\/mp\.weixin\.qq\.com\/mp\/getappmsgad
# Vsco
vsco.js = requires-body=1,max-size=0,script-path=https://raw.githubusercontent.com/NobyDa/Script/master/QuantumultX/File/vsco.js,type=http-response,pattern=^https?:\/\/vsco\.co\/api\/subscriptions\/2.1\/user-subscriptions\/
# 大片
dapian.js = requires-body=1,max-size=0,script-path=https://raw.githubusercontent.com/NobyDa/Script/master/QuantumultX/File/dapian.js,type=http-response,pattern=^https?:\/\/api\.vnision\.com\/v1\/(users\/|banners)
# Keep
Keep.js = requires-body=1,max-size=0,script-path=https://raw.githubusercontent.com/NobyDa/Script/master/Surge/JS/Keep.js,type=http-response,pattern=https:\/\/api\.gotokeep\.com\/(.+\/subject|.+\/dynamic)
# 扫描全能王
CamScanner.js = requires-body=1,max-size=0,script-path=https://raw.githubusercontent.com/NobyDa/Script/master/Surge/JS/CamScanner.js,type=http-response,pattern=https:\/\/(api|api-cs)\.intsig\.net\/purchase\/cs\/query_property\?

[MITM]
hostname = %APPEND% api.termius.com, mp.weixin.qq.com, vsco.co, api.vnision.com, api.gotokeep.com, ap*.intsig.net